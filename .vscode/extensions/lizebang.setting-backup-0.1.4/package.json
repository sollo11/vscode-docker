{
	"name": "setting-backup",
	"displayName": "Setting Backup",
	"description": "Setting Backup",
	"version": "0.1.4",
	"publisher": "lizebang",
	"icon": "icon.png",
	"author": {
		"name": "lizebang"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/banggua/vscode-setting-backup.git"
	},
	"bugs": {
		"url": "https://gitee.com/banggua/vscode-setting-backup/issues",
